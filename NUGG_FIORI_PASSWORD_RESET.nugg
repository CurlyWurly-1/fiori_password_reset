  <controller_definition COMPONENT_NAME="ZBC_C_PWD_RESET_UI" CONTROLLER_NAME="V_MAIN" VERSION="A" CONTROLLER_TYPE="01" CONTEXT="CONTEXT" DISPLAY_NAME="V_MAIN">
   <wdy_ctlr_compo COMPONENT_NAME="ZBC_C_PWD_RESET_UI" CONTROLLER_NAME="V_MAIN" CMPNAME="ONACTIONSUBMIT" VERSION="I" CMPTYPE="CL_WDY_MD_CTLR_EVENT_HANDLER" CMP_POSITION="0 " VISIBILITY="2" REF_CTLR_NAME="V_MAIN" REF_CMPNAME="SUBMIT" DISPLAY_NAME="ONACTIONSUBMIT" CODE_BODY="METHOD onactionsubmit .


  DATA lo_nd_user TYPE REF TO if_wd_context_node.

  DATA lo_el_user TYPE REF TO if_wd_context_element.
  DATA ls_user TYPE wd_this-&gt;element_user.

* get message manager
  DATA lo_api_controller     TYPE REF TO if_wd_controller.
  DATA lo_message_manager    TYPE REF TO if_wd_message_manager.
  DATA: lv_msgid        TYPE string,
        lv_message_text TYPE string,
        lv_url          TYPE string.

  DATA lo_componentcontroller TYPE REF TO ig_componentcontroller .
  lo_componentcontroller =   wd_this-&gt;get_componentcontroller_ctr( ).

  DATA lv_uuid TYPE sysuuid_c.

  DATA: ls_usr21     TYPE usr21,
        ls_adr6      TYPE adr6,
        lv_full_name TYPE adrp-name_text.

  DATA: lt_body        TYPE bcsy_text,
        ls_line        LIKE LINE OF lt_body,
        ls_line2       LIKE LINE OF lt_body,
        lv_subject     TYPE so_obj_des,
        lv_sender      TYPE ad_smtpadr,
        lv_receiver    TYPE ad_smtpadr,
        lv_send_result TYPE c.

  DATA: lt_match_result_tab TYPE match_result_tab,
        ls_match_result     TYPE match_result.

  lo_api_controller ?= wd_this-&gt;wd_get_api( ).

  CALL METHOD lo_api_controller-&gt;get_message_manager
    RECEIVING
      message_manager = lo_message_manager.

* navigate from &lt;CONTEXT&gt; to &lt;USER&gt; via lead selection
  lo_nd_user = wd_context-&gt;get_child_node( name = wd_this-&gt;wdctx_user ).

* @TODO handle non existant child
* IF lo_nd_user IS INITIAL.
* ENDIF.

* get element via lead selection
  lo_el_user = lo_nd_user-&gt;get_element( ).
* @TODO handle not set lead selection
  IF lo_el_user IS INITIAL.
  ENDIF.

* get all declared attributes
  lo_el_user-&gt;get_static_attributes(
    IMPORTING
      static_attributes = ls_user ).

  IF ls_user-username IS INITIAL OR
     ls_user-email IS INITIAL.

    MESSAGE e003(zbc_reset_password) INTO lv_message_text.
*   Please enter a valid value in each field!

*   report message
    CALL METHOD lo_message_manager-&gt;report_error_message
      EXPORTING
        message_text = lv_message_text
      RECEIVING
        message_id   = lv_msgid.

  ELSE.
    TRANSLATE ls_user-username TO UPPER CASE.
    SELECT SINGLE * FROM usr21 INTO ls_usr21
      WHERE bname EQ ls_user-username.
    IF sy-subrc EQ 0.
      SELECT * FROM adr6 INTO ls_adr6
        WHERE addrnumber EQ ls_usr21-addrnumber
          AND persnumber EQ ls_usr21-persnumber
          AND date_from  LE sy-datum
        ORDER BY date_from DESCENDING.
        EXIT.
      ENDSELECT.
      IF sy-subrc EQ 0.
        TRANSLATE ls_user-email TO UPPER CASE.
        TRANSLATE ls_adr6-smtp_addr TO UPPER CASE.

        IF ls_user-email EQ ls_adr6-smtp_addr.

          lv_uuid = lo_componentcontroller-&gt;set_auth_key(
            username = ls_user-username  ).

          SELECT SINGLE name_text FROM adrp INTO lv_full_name
            WHERE persnumber EQ ls_usr21-persnumber.

          MESSAGE s007(zbc_reset_password) INTO ls_line.
*         Dear
          CONCATENATE ls_line lv_full_name INTO ls_line SEPARATED BY space.
          CONCATENATE ls_line &apos;,&apos; INTO ls_line.
          APPEND ls_line TO lt_body.

          CLEAR ls_line.
          APPEND ls_line TO lt_body.

          CLEAR: ls_line, ls_line2.
          MESSAGE s008(zbc_reset_password) INTO ls_line2.
*       You have requested to reset your password.
          MESSAGE s009(zbc_reset_password) INTO ls_line.
*       Click the link below to reset your password.
          CONCATENATE ls_line2 &apos;  &apos; ls_line2 INTO ls_line RESPECTING BLANKS.
          APPEND ls_line TO lt_body.

          CLEAR ls_line.
          APPEND ls_line TO lt_body.


          CALL METHOD cl_wd_utilities=&gt;construct_wd_url
            EXPORTING
              application_name = &apos;ZBC_A_PWD_RESET_MD&apos;
            IMPORTING
              out_absolute_url = lv_url.

          FIND ALL OCCURRENCES OF &apos;/&apos; IN lv_url RESULTS lt_match_result_tab.
          READ TABLE lt_match_result_tab INTO ls_match_result INDEX 3.


          CASE sy-sysid.
            WHEN &apos;NPL&apos;.
              CONCATENATE &apos;https://vhcalnplci:44300&apos; lv_url+ls_match_result-offset INTO lv_url.
            WHEN &apos;DEV&apos;.
              CONCATENATE &apos;https://sapdev.medavie.bluecross.ca&apos; lv_url+ls_match_result-offset INTO lv_url.
            WHEN &apos;QEV&apos;.
              CONCATENATE &apos;https://sapqa.medavie.bluecross.ca&apos; lv_url+ls_match_result-offset INTO lv_url.
            WHEN &apos;PEV&apos;.
              CONCATENATE &apos;https://sap.medavie.bluecross.ca&apos; lv_url+ls_match_result-offset INTO lv_url.
            WHEN OTHERS.
          ENDCASE.

          CONCATENATE lv_url &apos;?action=y&amp;username=&apos; ls_user-username &apos;&amp;uuid=&apos; lv_uuid INTO ls_line.
          APPEND ls_line TO lt_body.

          CLEAR ls_line.
          APPEND ls_line TO lt_body.

          CLEAR ls_line.
          MESSAGE s010(zbc_reset_password) INTO ls_line.
*       If you did not request this, you can ignore this message.
          APPEND ls_line TO lt_body.

          CLEAR ls_line.
          APPEND ls_line TO lt_body.

          CLEAR ls_line.
          MESSAGE s011(zbc_reset_password) INTO ls_line.
*       This is a computer-generated email.  Please do not reply to this email.
          APPEND ls_line TO lt_body.

          MESSAGE s006(zbc_reset_password) INTO lv_subject.
*       Password Reset Request

          lv_receiver = ls_adr6-smtp_addr.
          lv_sender = &apos;NoReplyHCM@PeterMatthews&apos;.
          lo_componentcontroller-&gt;send_email_to_user(
            EXPORTING
              im_t_body =                       lt_body
              im_v_receiver =                   lv_receiver
              im_v_sender =                     lv_sender
              im_v_subject =                    lv_subject
            IMPORTING
              ex_send_result =                  lv_send_result
          ).

          wd_this-&gt;fire_to_confirmation_plg( ).
        ELSE.
          MESSAGE e005(zbc_reset_password) INTO lv_message_text.
*         Invalid email address!

*         report message
          CALL METHOD lo_message_manager-&gt;report_error_message
            EXPORTING
              message_text = lv_message_text
            RECEIVING
              message_id   = lv_msgid.
        ENDIF.
      ELSE.
        CLEAR lv_message_text.
        MESSAGE e005(zbc_reset_password) INTO lv_message_text.
*       Invalid email address!

        CALL METHOD lo_message_manager-&gt;report_error_message
          EXPORTING
            message_text = lv_message_text
          RECEIVING
            message_id   = lv_msgid.

      ENDIF.
    ELSE.

*     report message
      CLEAR lv_message_text.
      MESSAGE e004(zbc_reset_password) INTO lv_message_text.
*     Invalid user!

      CALL METHOD lo_message_manager-&gt;report_error_message
        EXPORTING
          message_text = lv_message_text
        RECEIVING
          message_id   = lv_msgid.

    ENDIF.
  ENDIF.



ENDMETHOD.
