   <wdy_ctlr_compo COMPONENT_NAME="ZBC_C_PWD_RESET_MD" CONTROLLER_NAME="COMPONENTCONTROLLER" CMPNAME="VERIFY_AUTH_FROM_TABLE" VERSION="I" CMPTYPE="CL_WDY_MD_CONTROLLER_METHOD" CMP_POSITION="0 " VISIBILITY="2" DISPLAY_NAME="VERIFY_AUTH_FROM_TABLE" CODE_BODY="METHOD verify_auth_from_table .

  DATA: ls_zbc_reset_pwd TYPE zbc_reset_pwd.

  CLEAR ex_valid.
  SELECT * FROM zbc_reset_pwd INTO ls_zbc_reset_pwd
    WHERE authdate EQ im_authdate AND
          uname    EQ im_uname AND
          uuid     EQ im_uuid.
    ex_valid = abap_true.
  ENDSELECT.

ENDMETHOD.
