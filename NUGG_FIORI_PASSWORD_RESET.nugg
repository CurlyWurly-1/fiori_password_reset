  <controller_definition COMPONENT_NAME="ZBC_C_PWD_RESET_UI" CONTROLLER_NAME="COMPONENTCONTROLLER" VERSION="A" CONTROLLER_TYPE="02" CONTEXT="CONTEXT" DISPLAY_NAME="COMPONENTCONTROLLER" RUNTIME_OBJECT="ZIWCI_BC_C_PWD_RESET_UI">
   <wdy_ctlr_compo COMPONENT_NAME="ZBC_C_PWD_RESET_UI" CONTROLLER_NAME="COMPONENTCONTROLLER" CMPNAME="SEND_EMAIL_TO_USER" VERSION="I" CMPTYPE="CL_WDY_MD_CONTROLLER_METHOD" CMP_POSITION="0 " VISIBILITY="2" DISPLAY_NAME="SEND_EMAIL_TO_USER" CODE_BODY="METHOD send_email_to_user .

  DATA:
    lv_send_result TYPE c,

    lo_sender      TYPE REF TO if_sender_bcs,
    lo_receiver    TYPE REF TO if_recipient_bcs,

    lo_email       TYPE REF TO cl_bcs,
    lo_email_body  TYPE REF TO cl_document_bcs,

    lx_exception   TYPE REF TO cx_bcs.


  TRY.
      lo_email = cl_bcs=&gt;create_persistent( ).

      lo_email_body = cl_document_bcs=&gt;create_document(
      i_type = &apos;RAW&apos;
      i_text = im_t_body
      i_subject = im_v_subject ).

      lo_email-&gt;set_document( lo_email_body ).

      lo_sender = cl_cam_address_bcs=&gt;create_internet_address( im_v_sender ).

      lo_receiver = cl_cam_address_bcs=&gt;create_internet_address( im_v_receiver ).
      lo_email-&gt;add_recipient( i_recipient = lo_receiver
      i_express = &apos;X&apos; ).

      lo_email-&gt;set_sender( i_sender = lo_sender ).


      lo_email-&gt;set_send_immediately( &apos;X&apos; ).

      lo_email-&gt;send( EXPORTING
      i_with_error_screen = &apos;X&apos;
      RECEIVING
      result = lv_send_result ).

      ex_send_result = lv_send_result.

      COMMIT WORK.

    CATCH cx_bcs INTO lx_exception.

  ENDTRY.

ENDMETHOD.
