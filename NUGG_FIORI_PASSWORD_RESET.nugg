   <wdy_ctlr_compo COMPONENT_NAME="ZBC_C_PWD_RESET_UI" CONTROLLER_NAME="COMPONENTCONTROLLER" CMPNAME="SET_AUTH_KEY" VERSION="I" CMPTYPE="CL_WDY_MD_CONTROLLER_METHOD" CMP_POSITION="0 " VISIBILITY="2" DISPLAY_NAME="SET_AUTH_KEY" CODE_BODY="METHOD set_auth_key .

  DATA: ls_line  TYPE zbc_reset_pwd,
        lv_uname TYPE xubname.

  lv_uname = username.

  SET LOCALE LANGUAGE &apos;E&apos;.

  TRANSLATE lv_uname TO UPPER CASE.

  SET LOCALE LANGUAGE &apos; &apos;.

  SELECT SINGLE * INTO ls_line

    FROM zbc_reset_pwd

    WHERE authdate EQ sy-datum
      AND uname    EQ lv_uname.

  IF sy-subrc = 0.  &quot;Record already exist
    uuid = ls_line-uuid.
  ELSE.                  &quot;Record not exist; Create record and UUID

    ls_line-uname = lv_uname.

    CALL FUNCTION &apos;SYSTEM_UUID_C_CREATE&apos;
      IMPORTING
        uuid = ls_line-uuid.

    IF ls_line-uuid IS INITIAL.

    ENDIF.

    ls_line-authdate = sy-datum.
    MODIFY zbc_reset_pwd FROM ls_line.

    IF sy-subrc &lt;&gt; 0.

    ELSE.
      uuid = ls_line-uuid.
    ENDIF.

  ENDIF.


ENDMETHOD.
