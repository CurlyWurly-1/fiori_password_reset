    <wdy_ctlr_compo COMPONENT_NAME="ZBC_C_PWD_RESET_MD" CONTROLLER_NAME="W_MAIN" CMPNAME="HANDLEDEFAULT" VERSION="I" CMPTYPE="CL_WDY_MD_CTLR_EVENT_HANDLER" CMP_POSITION="0 " VISIBILITY="2" REF_VIEW_NAME="W_MAIN" REF_PLUG_NAME="DEFAULT" DISPLAY_NAME="HANDLEDEFAULT" CODE_BODY="METHOD handledefault .


  DATA: lv_action        TYPE c,
        lv_username      TYPE xubname,
        lv_uuid          TYPE sysuuid-c,
        lv_valid         TYPE boolean,
        lv_message       TYPE  natxt,
        lv_password      TYPE  char8,
        lv_subrc         TYPE  syst_subrc,
        lv_e_message_l1  TYPE natxt,
        lv_e_message_l2  TYPE natxt,
        lv_s_message_l1  TYPE natxt,
        lv_s_message_l2  TYPE natxt,
        lv_s_message_l3  TYPE natxt,
        lv_m_message_l1  TYPE natxt,
        lv_m_message_l2  TYPE natxt,
        lv_m_message_l3  TYPE natxt,
        lv_m_message_l4  TYPE natxt,
        lv_m_message_l5  TYPE natxt,
        lv_email_address TYPE ad_smtpadr,
        lv_fullname      TYPE ad_namtext,
        lv_subject       TYPE so_obj_des.

  DATA lo_componentcontroller TYPE REF TO ig_componentcontroller .
  lo_componentcontroller =   wd_this-&gt;get_componentcontroller_ctr( ).

  DATA lo_nd_success_messages TYPE REF TO if_wd_context_node.

  DATA lo_el_success_messages TYPE REF TO if_wd_context_element.
  DATA ls_success_messages TYPE wd_this-&gt;element_success_messages.

* navigate from &lt;CONTEXT&gt; to &lt;SUCCESS_MESSAGES&gt; via lead selection
  lo_nd_success_messages = wd_context-&gt;get_child_node( name = wd_this-&gt;wdctx_success_messages ).
* get element via lead selection
  lo_el_success_messages = lo_nd_success_messages-&gt;get_element( ).

  DATA lo_nd_email_message TYPE REF TO if_wd_context_node.

  DATA lo_el_email_message TYPE REF TO if_wd_context_element.
  DATA ls_email_message TYPE wd_this-&gt;element_email_message.
  DATA lv_sender TYPE wd_this-&gt;element_email_message-sender.

* navigate from &lt;CONTEXT&gt; to &lt;EMAIL_MESSAGE&gt; via lead selection
  lo_nd_email_message = wd_context-&gt;get_child_node( name = wd_this-&gt;wdctx_email_message ).
* get element via lead selection
  lo_el_email_message = lo_nd_email_message-&gt;get_element( ).


  DATA lo_nd_error_messages TYPE REF TO if_wd_context_node.

  DATA lo_el_error_messages TYPE REF TO if_wd_context_element.
  DATA ls_error_messages TYPE wd_this-&gt;element_error_messages.

* navigate from &lt;CONTEXT&gt; to &lt;ERROR_MESSAGES&gt; via lead selection
  lo_nd_error_messages = wd_context-&gt;get_child_node( name = wd_this-&gt;wdctx_error_messages ).
* get element via lead selection
  lo_el_error_messages = lo_nd_error_messages-&gt;get_element( ).

  lv_action   = action.
  lv_username = username.
  lv_uuid     = uuid.

  &quot;Check whether it&apos;s Reset Request from email
  IF lv_action IS NOT INITIAL
    AND lv_username IS NOT INITIAL
    AND lv_uuid IS NOT INITIAL.

    &quot;Verify authenticity of Reset Request
    lo_componentcontroller-&gt;verify_auth_from_table(
      EXPORTING
        im_authdate = sy-datum
        im_uname    = lv_username
        im_uuid     = lv_uuid
      IMPORTING
        ex_valid    = lv_valid
    ).


    IF lv_valid = abap_true. &quot;Password Reset Request verified
      SET LOCALE LANGUAGE &apos;E&apos;.
      TRANSLATE lv_action TO UPPER CASE.
      SET LOCALE LANGUAGE &apos; &apos;.
      IF lv_action = &apos;Y&apos;.
*       reset password
        lo_componentcontroller-&gt;reset_password(
          EXPORTING
            im_username = lv_username
          IMPORTING
            ex_message  = lv_message
            ex_password = lv_password
            ex_subrc    = lv_subrc
        ).

        IF lv_subrc NE 0.
*         Error messages
          lv_e_message_l1 = lv_message.
          MESSAGE e016(zbc_reset_password) INTO lv_e_message_l2.
*         Please contact your system administrator.

          ls_error_messages-line01 = lv_e_message_l1.
          ls_error_messages-line02 = lv_e_message_l2.

*         set all declared attributes
          lo_el_error_messages-&gt;set_static_attributes(
             static_attributes = ls_error_messages ).

          wd_this-&gt;fire_to_error_plg(
          ).

        ELSE.
*         Success Messages
          MESSAGE s017(zbc_reset_password) INTO lv_s_message_l1.
*         Your password has been reset.
          MESSAGE s018(zbc_reset_password) INTO lv_s_message_l2.
*         A temporary password has been sent to your email address.
          MESSAGE s019(zbc_reset_password) INTO lv_s_message_l3.
*         Please check your email for further instructions.

          ls_success_messages-line01 = lv_s_message_l1.
          ls_success_messages-line02 = lv_s_message_l2.
          ls_success_messages-line03 = lv_s_message_l3.

*         set all declared attributes
          lo_el_success_messages-&gt;set_static_attributes(
             static_attributes = ls_success_messages ).

*         Email Message

          lo_componentcontroller-&gt;get_email_parameters(
            EXPORTING
              im_username =        lv_username
            IMPORTING
              ex_email_address =   lv_email_address
              ex_fullname =        lv_fullname
          ).

          MESSAGE s007(zbc_reset_password) INTO lv_m_message_l1.
*         Dear
          CONCATENATE lv_m_message_l1 lv_fullname INTO lv_m_message_l1 SEPARATED BY space.
          CONCATENATE lv_m_message_l1 &apos;,&apos; INTO lv_m_message_l1.

          MESSAGE s020(zbc_reset_password) INTO lv_m_message_l2.
*         As per your request, please find below your temporary password.

          lv_m_message_l3 = lv_password.

          MESSAGE s021(zbc_reset_password) INTO lv_m_message_l4.
*         After logging in, you will be asked to choose a new password.
          MESSAGE s011(zbc_reset_password) INTO lv_m_message_l5.
*         This is a computer-generated email.  Please do not reply to this email.

*         get single attribute
          lo_el_email_message-&gt;get_attribute(
            EXPORTING
              name =  `SENDER`
            IMPORTING
              value = lv_sender ).

          MESSAGE s014(zbc_reset_password) INTO lv_subject.
*         Password Reset Confirmation

          ls_email_message-sender  	  = lv_sender.
          ls_email_message-recipient  = lv_email_address.
          ls_email_message-subject    = lv_subject.
          ls_email_message-line01  	  = lv_m_message_l1.
          ls_email_message-line02  	  = lv_m_message_l2.
          ls_email_message-line03  	  = lv_m_message_l3.
          ls_email_message-line04  	  = lv_m_message_l4.
          ls_email_message-line05  	  = lv_m_message_l5.

*         set all declared attributes
          lo_el_email_message-&gt;set_static_attributes(
             static_attributes = ls_email_message ).

*         go to confirmation page and send email
          wd_this-&gt;fire_to_confirmation_plg(
          ).

        ENDIF.



      ENDIF.


    ELSE. &quot;Reset Request verify wrong
*     Error messages

      MESSAGE e022(zbc_reset_password) INTO lv_e_message_l1.
*     Your request has expired.  Please start again.

      ls_error_messages-line01 = lv_e_message_l1.
      ls_error_messages-line02 = space.

*     set all declared attributes
      lo_el_error_messages-&gt;set_static_attributes(
         static_attributes = ls_error_messages ).

      wd_this-&gt;fire_to_error_plg(
          ).
    ENDIF.


  ENDIF.



ENDMETHOD.
