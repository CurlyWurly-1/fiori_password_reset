    <wdy_ctlr_compo COMPONENT_NAME="ZBC_C_PWD_RESET_MD" CONTROLLER_NAME="V_CONFIRMATION" CMPNAME="HANDLEFROM_MAIN" VERSION="I" CMPTYPE="CL_WDY_MD_CTLR_EVENT_HANDLER" CMP_POSITION="0 " VISIBILITY="2" REF_VIEW_NAME="V_CONFIRMATION" REF_PLUG_NAME="FROM_MAIN" DISPLAY_NAME="HANDLEFROM_MAIN" CODE_BODY="METHOD handlefrom_main .

  DATA: lt_body        TYPE bcsy_text,
        ls_line        LIKE LINE OF lt_body,
        ls_line2       LIKE LINE OF lt_body,
        lv_subject     TYPE so_obj_des,
        lv_sender      TYPE ad_smtpadr,
        lv_receiver    TYPE ad_smtpadr,
        lv_send_result TYPE c.

  DATA lo_componentcontroller TYPE REF TO ig_componentcontroller .
  lo_componentcontroller =   wd_this-&gt;get_componentcontroller_ctr( ).


  DATA lo_nd_email_message TYPE REF TO if_wd_context_node.

  DATA lo_el_email_message TYPE REF TO if_wd_context_element.
  DATA ls_email_message TYPE wd_this-&gt;element_email_message.

* navigate from &lt;CONTEXT&gt; to &lt;EMAIL_MESSAGE&gt; via lead selection
  lo_nd_email_message = wd_context-&gt;get_child_node( name = wd_this-&gt;wdctx_email_message ).


* get element via lead selection
  lo_el_email_message = lo_nd_email_message-&gt;get_element( ).


* get all declared attributes
  lo_el_email_message-&gt;get_static_attributes(
    IMPORTING
      static_attributes = ls_email_message ).

* Send email message
  lv_sender = ls_email_message-sender.
  lv_receiver = ls_email_message-recipient.
  lv_subject = ls_email_message-subject.

  CLEAR ls_line.
  ls_line = ls_email_message-line01.
  APPEND ls_line TO lt_body.

  CLEAR ls_line.
  APPEND ls_line TO lt_body.

  CLEAR ls_line.
  ls_line = ls_email_message-line02.
  APPEND ls_line TO lt_body.

  CLEAR ls_line.
  APPEND ls_line TO lt_body.

  CLEAR ls_line.
  ls_line = ls_email_message-line03.
  APPEND ls_line TO lt_body.

  CLEAR ls_line.
  APPEND ls_line TO lt_body.

  CLEAR ls_line.
  ls_line = ls_email_message-line04.
  APPEND ls_line TO lt_body.

  CLEAR ls_line.
  APPEND ls_line TO lt_body.

  CLEAR ls_line.
  ls_line = ls_email_message-line05.
  APPEND ls_line TO lt_body.

  lo_componentcontroller-&gt;send_email_to_user(
    EXPORTING
      im_t_body =                       lt_body
      im_v_receiver =                   lv_receiver
      im_v_sender =                     lv_sender
      im_v_subject =                    lv_subject
    IMPORTING
      ex_send_result =                  lv_send_result
  ).

ENDMETHOD.
