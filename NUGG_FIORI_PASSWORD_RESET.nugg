  <controller_definition COMPONENT_NAME="ZBC_C_PWD_RESET_MD" CONTROLLER_NAME="COMPONENTCONTROLLER" VERSION="A" CONTROLLER_TYPE="02" CONTEXT="CONTEXT" DISPLAY_NAME="COMPONENTCONTROLLER" RUNTIME_OBJECT="ZIWCI_BC_C_PWD_RESET_MD">
   <wdy_ctlr_compo COMPONENT_NAME="ZBC_C_PWD_RESET_MD" CONTROLLER_NAME="COMPONENTCONTROLLER" CMPNAME="GET_EMAIL_PARAMETERS" VERSION="I" CMPTYPE="CL_WDY_MD_CONTROLLER_METHOD" CMP_POSITION="0 " VISIBILITY="2" DISPLAY_NAME="GET_EMAIL_PARAMETERS" CODE_BODY="METHOD get_email_parameters .

  DATA: ls_usr21     TYPE usr21,
        ls_adr6      TYPE adr6,
        lv_full_name TYPE adrp-name_text.

  SELECT SINGLE * FROM usr21 INTO ls_usr21
    WHERE bname EQ im_username.
  IF sy-subrc EQ 0.
    SELECT * FROM adr6 INTO ls_adr6
      WHERE addrnumber EQ ls_usr21-addrnumber
        AND persnumber EQ ls_usr21-persnumber
        AND date_from  LE sy-datum
      ORDER BY date_from DESCENDING.
      EXIT.
    ENDSELECT.
    IF sy-subrc EQ 0.
      MOVE ls_adr6-smtp_addr TO ex_email_address.
      SELECT SINGLE name_text FROM adrp INTO ex_fullname
        WHERE persnumber EQ ls_usr21-persnumber.
    ENDIF.
  ENDIF.

ENDMETHOD.
" ACTION_TYPE="0" ABAP_TYPING="0"/>
   <wdy_ctlr_compo COMPONENT_NAME="ZBC_C_PWD_RESET_MD" CONTROLLER_NAME="COMPONENTCONTROLLER" CMPNAME="RESET_PASSWORD" VERSION="I" CMPTYPE="CL_WDY_MD_CONTROLLER_METHOD" CMP_POSITION="0 " VISIBILITY="2" DISPLAY_NAME="RESET_PASSWORD" CODE_BODY="METHOD reset_password .

  DATA: lv_username TYPE bapibname-bapibname,
        lv_random   TYPE c LENGTH 8,
        lv_password TYPE bapipwd,
        lt_return   TYPE STANDARD TABLE OF bapiret2,
        ls_return   TYPE bapiret2.

  lv_username = im_username.

* Generate a random password of length 8
  CALL FUNCTION &apos;RSEC_GENERATE_PASSWORD&apos;
    EXPORTING
      alphabet_length = 8
      output_length   = 8
    IMPORTING
      output          = lv_random
    EXCEPTIONS
      some_error      = 1
      OTHERS          = 2.

  IF sy-subrc NE 0.
    ex_subrc = 1. &quot;Error generating a random password.
    MESSAGE e015(zbc_reset_password) INTO ex_message.
*   Error generating a random password.

    EXIT.
  ENDIF.

  ex_password = lv_random.
  lv_password = lv_random.

  CALL FUNCTION &apos;BAPI_USER_CHANGE&apos;
    EXPORTING
      username  = lv_username
      password  = lv_password
      passwordx = &apos;X&apos;
    TABLES
      return    = lt_return.

  READ TABLE lt_return INTO ls_return INDEX 1.
  IF ls_return-type NE &apos;S&apos;.
    ex_subrc = 2.
    ex_message = ls_return-message.
    CLEAR ex_password.
  ENDIF.


ENDMETHOD.
" ACTION_TYPE="0" ABAP_TYPING="0"/>
   <wdy_ctlr_compo COMPONENT_NAME="ZBC_C_PWD_RESET_MD" CONTROLLER_NAME="COMPONENTCONTROLLER" CMPNAME="SEND_EMAIL_TO_USER" VERSION="I" CMPTYPE="CL_WDY_MD_CONTROLLER_METHOD" CMP_POSITION="0 " VISIBILITY="2" DISPLAY_NAME="SEND_EMAIL_TO_USER" CODE_BODY="METHOD send_email_to_user .

  DATA:
    lv_send_result TYPE c,

    lo_sender      TYPE REF TO if_sender_bcs,
    lo_receiver    TYPE REF TO if_recipient_bcs,

    lo_email       TYPE REF TO cl_bcs,
    lo_email_body  TYPE REF TO cl_document_bcs,

    lx_exception   TYPE REF TO cx_bcs.


  TRY.
      lo_email = cl_bcs=&gt;create_persistent( ).

      lo_email_body = cl_document_bcs=&gt;create_document(
      i_type = &apos;RAW&apos;
      i_text = im_t_body
      i_subject = im_v_subject ).

      lo_email-&gt;set_document( lo_email_body ).

      lo_sender = cl_cam_address_bcs=&gt;create_internet_address( im_v_sender ).

      lo_receiver = cl_cam_address_bcs=&gt;create_internet_address( im_v_receiver ).
      lo_email-&gt;add_recipient( i_recipient = lo_receiver
      i_express = &apos;X&apos; ).

      lo_email-&gt;set_sender( i_sender = lo_sender ).


      lo_email-&gt;set_send_immediately( &apos;X&apos; ).

      lo_email-&gt;send( EXPORTING
      i_with_error_screen = &apos;X&apos;
      RECEIVING
      result = lv_send_result ).

      ex_send_result = lv_send_result.

      COMMIT WORK.

    CATCH cx_bcs INTO lx_exception.

  ENDTRY.

ENDMETHOD.
